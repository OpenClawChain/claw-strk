    "url": "https://github.com/OpenClawChain/claw-strk.git"
  },
  "bugs": {
    "url": "https://github.com/OpenClawChain/claw-strk/issues"
  },
  "homepage": "https://github.com/OpenClawChain/claw-strk#readme",
  "license": "MIT",
  "packageManager": "pnpm@10.28.2",
  "dependencies": {
    "@avnu/avnu-sdk": "^4.0.1",
    "commander": "^14.0.3",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "starknet": "^8.9.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
